{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "GOOGLE_SERVICES_INFOPLIST": "./GoogleService-Info-2.plist",
        "GOOGLE_SERVICES_JSON": "./google-services-2.json"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_INFOPLIST": "./GoogleService-Info-2.plist",
        "GOOGLE_SERVICES_JSON": "./google-services-2.json"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
